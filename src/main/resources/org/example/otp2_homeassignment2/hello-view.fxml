<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="266.0" prefWidth="373.0" spacing="20.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.otp2_homeassignment2.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <children>
        <Button fx:id="button1" mnemonicParsing="false" onAction="#setEN" prefHeight="26.0" prefWidth="270.0" text="%button1.text" />
        <Button fx:id="button2" mnemonicParsing="false" onAction="#setIR" prefHeight="26.0" prefWidth="270.0" text="%button2.text" />
        <Button fx:id="button3" mnemonicParsing="false" onAction="#setJP" prefHeight="26.0" prefWidth="267.0" text="%button3.text" />
        <Label fx:id="label" text="%label.text"/>
    </children>
</VBox>
